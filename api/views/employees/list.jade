<%- include ../layouts/header.ejs %>
<div class="page-data">
    <div class="data-table">
        <table>
            <tr>
                <th width="50px">No</th>
                <th>id</th>
                <th>name</th>
            </tr>

            <% if(data.length){
            for(var i = 0;i < data.length;i++) { %>
            <tr>
                <td><%=data[i].id%></td>
                <td><%=data[i].name%></td>

            </tr>
            <% }
            }else{ %>
            <tr>
                <td colspan="6">No user</td>
            </tr>
            <% } %>
        </table>
    </div>
</div>

<%- include ../layouts/footer.ejs %>